[global]
daemonize = no
error_log = "{{MALT_DIR}}/logs/php-fpm_{{PORT}}.log"

[www]
listen = 127.0.0.1:{{PORT}}
pm = dynamic
pm.max_children = 20
pm.start_servers = 5
pm.min_spare_servers = 3
pm.max_spare_servers = 7
access.log = "{{MALT_DIR}}/logs/php-fpm-access_{{PORT}}.log"
slowlog = "{{MALT_DIR}}/logs/php-fpm-slow_{{PORT}}.log"